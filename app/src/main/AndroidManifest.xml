        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.classassessment.ExampleActivity"></activity>
        <activity android:name="com.example.classassessment.FlagAppActivity" />
