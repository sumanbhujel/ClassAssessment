        <activity android:name="com.example.classassessment.SharedPreActivity" />
        <activity android:name="com.example.classassessment.FileActivity" />
        <activity android:name="com.example.classassessment.ToolbarActivity" />
        <activity android:name="com.example.classassessment.TabbedActivity" />
        <activity android:name="com.example.classassessment.FragmentActivity" />
        <activity android:name="com.example.classassessment.SplashActivity" />
        <activity android:name="com.example.classassessment.RecyclerViewActivity" />
        <activity android:name="com.example.classassessment.UserDetailActivity" />
        <activity android:name="com.example.classassessment.UserListView" />
        <activity android:name="com.example.classassessment.FrameLayoutActivity" />
        <activity android:name="com.example.classassessment.TableLayoutActivity" />
        <activity android:name="com.example.classassessment.LayoutsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.classassessment.ShowActivity" />
        <activity android:name="com.example.classassessment.RegdataActivity" />
        <activity android:name="com.example.classassessment.TaskTwoActivity" />
        <activity android:name="com.example.classassessment.TaskThreeActivity" />
